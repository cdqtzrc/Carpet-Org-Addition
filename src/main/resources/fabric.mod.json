{
  "schemaVersion": 1,
  "id": "carpet-org-addition",
  "version": "${version}",
  "name": "Carpet Org Addition",
  "description": "一个非常Not Vanilla的Carpet扩展模组，提供了一些实用和无用的功能，\n默认情况下不会改变游戏的任何机制，所有功能都需要手动开启。",
  "authors": [
    "cdqtzrc"
  ],
  "contact": {
    "sources": "https://github.com/cdqtzrc/Carpet-Org-Addition"
  },
  "license": "MIT",
  "icon": "assets/carpet-org-addition/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.carpet_org_addition.client.CarpetOrgAdditionClient"
    ],
    "main": [
      "org.carpet_org_addition.CarpetOrgAddition"
    ]
  },
  "mixins": [
    "carpet_org.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}"
  }
}
